<%- include('./partials/header.ejs') %>
<body>
    <%- include('./partials/navbar.ejs') %>
    <div class="container">
        <% servicos.forEach(servico => { %>
            <div class="card">
                <div class="card-header">
                    <h3><%= servico.val().nome %></h3>
                </div>
                <div class="card-body">
                    <h4>Ip:</h4>
                    <h5><%= servico.val().ip %></h5>
                    <br>
                    <h4>Porta:</h4>
                    <h5><%= servico.val().porta%></h5>
                </div>
            </div>
        <% }) %>
    </div>
</body>
<%- include('./partials/footer.ejs') %>